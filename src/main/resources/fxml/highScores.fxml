<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #6b3e2e;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="knightschess.javafx.controller.HighScoresController">
    <children>
        <TableView fx:id="tableView" layoutX="125.0" layoutY="82.0" prefHeight="253.0" prefWidth="376.0">
            <columns>
                <TableColumn fx:id="player" prefWidth="188.0" text="Player" />
                <TableColumn fx:id="score" minWidth="6.0" prefWidth="187.0" text="Score" />
            </columns>
        </TableView>
        <Label alignment="CENTER" layoutX="125.0" layoutY="45.0" prefHeight="37.0" prefWidth="376.0" style="-fx-background-color: #ffffff;" text="High Scores" textFill="#6f5050">
            <font>
                <Font name="SansSerif Regular" size="18.0" />
            </font>
        </Label>
        <Button fx:id="backButton" layoutX="287.0" layoutY="351.0" mnemonicParsing="false" onAction="#backAction" text="Back" />
    </children>
</AnchorPane>
